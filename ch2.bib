@article{mitalDownscaledHyperresolution4002022,
  title = {Downscaled Hyper-Resolution (400\&thinsp;m) Gridded Datasets of Daily Precipitation and Temperature (2008\textendash 2019) for the {{East}}\textendash{{Taylor}} Subbasin (Western {{United States}})},
