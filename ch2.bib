@article{molotchPhysiographicClimaticControls2014,
  title = {Physiographic and Climatic Controls on Snow Cover Persistence in the {{Sierra Nevada Mountains}}: {{SNOW PERSISTENCE IN THE SIERRA NEVADA}}},
