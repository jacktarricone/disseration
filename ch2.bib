}

@article{ombadiWarminginducedReductionSnow2023,
  title = {A Warming-Induced Reduction in Snow Fraction Amplifies Rainfall Extremes},
  author = {Ombadi, Mohammed and Risser, Mark D. and Rhoades, Alan M. and Varadharajan, Charuleka},
  year = {2023},
