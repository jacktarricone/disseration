@article{hatchettMidwinterDrySpells2023,
  title = {Midwinter {{Dry Spells Amplify Post}}-{{Fire Snowpack Decline}}},
  author = {Hatchett, Benjamin J. and Koshkin, Arielle L. and Guirguis, Kristen and Rittger, Karl and Nolin, Anne W. and Heggli, Anne and Rhoades, Alan M. and East, Amy E. and Siirila-Woodburn, Erica R. and Brandt, W. Tyler and Gershunov, Alexander and Haleakala, Kayden},
