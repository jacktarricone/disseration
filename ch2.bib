@article{trujilloSnowpackRegimesWestern2014,
  title = {Snowpack Regimes of the {{Western United States}}},
  author = {Trujillo, Ernesto and Molotch, Noah P.},
  year = {2014},
  month = jul,
  journal = {Water Resources Research},
  volume = {50},
