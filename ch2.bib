@article{luceSensitivitySnowpackStorage2014,
  title = {Sensitivity of Snowpack Storage to Precipitation and Temperature Using Spatial and Temporal Analog Models},
  author = {Luce, Charles H. and {Lopez-Burgos}, Viviana and Holden, Zachary},
  year = {2014},
