@article{crumleySnowCloudMetricsSnowInformation2020,
  title = {{{SnowCloudMetrics}}: {{Snow Information}} for {{Everyone}}},
  shorttitle = {{{SnowCloudMetrics}}},
  author = {Crumley, Ryan L. and Palomaki, Ross T. and Nolin, Anne W. and Sproles, Eric A. and Mar, Eugene J.},
  year = {2020},
  month = oct,
  journal = {Remote Sensing},
  volume = {12},
