  shorttitle = {Correlation {{Coefficients}}},
  author = {Schober, Patrick and Boer, Christa and Schwarte, Lothar A.},
  year = {2018},
  month = may,
  journal = {Anesthesia \& Analgesia},
  volume = {126},
