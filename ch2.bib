@article{shakespeareClearskyDownwellingLongwave2021,
  title = {The Clear-Sky Downwelling Long-Wave Radiation at the Surface in Current and Future Climates},
  author = {Shakespeare, Callum J. and Roderick, Michael L.},
  year = {2021},
  journal = {Quarterly Journal of the Royal Meteorological Society},
  volume = {147},
