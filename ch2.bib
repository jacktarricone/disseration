@article{pepinTemperatureTrendsHigh2008a,
  title = {Temperature Trends at High Elevations: {{Patterns}} across the Globe},
  shorttitle = {Temperature Trends at High Elevations},
  author = {Pepin, N. C. and Lundquist, J. D.},
