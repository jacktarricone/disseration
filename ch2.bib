  keywords = {canopy cover,insolation,solar},
  file = {/Users/jacktarricone/Zotero/storage/WB32AZML/Zou et al. - 2007 - Effects of topography and woody plant canopy cover.pdf;/Users/jacktarricone/Zotero/storage/XSR368QV/2007GL031484.html}
