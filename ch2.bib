  file = {/Users/jacktarricone/Zotero/storage/TM9ZHZYG/Hatchett et al. - 2023 - Midwinter Dry Spells Amplify Post‐Fire Snowpack De.pdf}
}

@article{hatchettMonitoringDailyEvolution2022,
  title = {Monitoring the Daily Evolution and Extent of Snow Drought},
  author = {Hatchett, Benjamin J. and Rhoades, Alan M. and McEvoy, Daniel J.},
  year = {2022},
  month = mar,
  journal = {Natural Hazards and Earth System Sciences},
  volume = {22},
