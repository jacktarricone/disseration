@article{haleDriversSpatiotemporalPatterns2023,
  title = {Drivers of Spatiotemporal Patterns of Surface Water Inputs in a Catchment at the Rain-Snow Transition Zone of the Water-Limited Western {{United States}}},
  author = {Hale, K. and Kiewiet, L. and Trujillo, E. and Krohe, C. and Hedrick, A. and Marks, D. and Kormos, P. and Havens, S. and McNamara, J. and Link, T. and Godsey, S. E.},
  year = {2023},
  month = jan,
  journal = {Journal of Hydrology},
  volume = {616},
  pages = {128699},
  issn = {0022-1694},
  doi = {10.1016/j.jhydrol.2022.128699},
  url = {https://www.sciencedirect.com/science/article/pii/S0022169422012690},
  urldate = {2023-06-27},
  abstract = {Spatial and temporal dynamics of rainfall and snowmelt (i.e., surface water inputs, SWI) control soil moisture, groundwater recharge, and streamflow at annual, seasonal, and event scales. In the rain-snow transition zone, comprising a large portion of the mountainous western United States, there is limited understanding of the sensitivity of spatiotemporal SWI dynamics across hydrologically variable water years (WYs). We modeled rainfall and snowpack dynamics in a small headwater catchment (1.8~km2) spanning the rain-snow transition in southwestern Idaho, USA, for two hydrologically distinct WYs (2011 and 2014). In wet WY 2011 and dry WY 2014, total precipitation drove spatial variability in annual SWI. Snow drifts generated more SWI (901\textendash 2080~mm) than high-elevation scour zones (442\textendash 640~mm), which generated less SWI than mid-elevation, non-drift locations (452\textendash 784~mm). Seasonally, energy fluxes differed most during the snowmelt period, where higher net radiation at lower elevations and south-facing slopes drove SWI production. At the rain-on-snow (ROS) event scale, higher elevations and north-facing slopes generated 15\textendash 20~\% of annual SWI, due mainly to higher turbulent fluxes. The most productive ROS events occurred after peak snow water equivalent (SWE), when rainfall fell onto ripe snowpacks. Snow drift locations were less susceptible to melt during ROS events, offset by the larger cold content and snowpack mass. Thus, catchment water resources depend on SWI magnitude, location, and timing, which are moderated by drift persistence at all temporal scales. As the climate warms, shifts in spatiotemporal SWI distribution are expected with declines in snowfall and snowfall redistribution in this area.},
  langid = {english},
  keywords = {Climate warming,Energy balance,Hydrological modeling,Rain-on-snow events,Recharge,Snow,Snowmelt,Water balance},
  file = {/Users/jacktarricone/Zotero/storage/APBGA62U/Hale et al. - 2022 - Drivers of spatiotemporal patterns of surface wate.pdf;/Users/jacktarricone/Zotero/storage/UVWS5FFU/Hale et al. - 2023 - Drivers of spatiotemporal patterns of surface wate.pdf}
}

@article{haleModelingStreamflowSensitivity2022,
  title = {Modeling Streamflow Sensitivity to Climate Warming and Surface Water Inputs in a Montane Catchment},
  author = {Hale, K. E. and Wlostowski, A. N. and Badger, A. M. and Musselman, K. N. and Livneh, B. and Molotch, N. P.},
  year = {2022},
  month = feb,
  journal = {Journal of Hydrology: Regional Studies},
  volume = {39},
  pages = {100976},
  issn = {2214-5818},
  doi = {10.1016/j.ejrh.2021.100976},
  url = {https://www.sciencedirect.com/science/article/pii/S2214581821002056},
  urldate = {2023-06-27},
  abstract = {Study region Gordon Gulch, an upper-montane forest watershed in the Colorado Front Range. Study focus As the climate warms, the fraction of precipitation falling as snow is expected to decrease and the timing of snowmelt is expected to shift earlier in spring. In snow-dominated regions, these changes in snow accumulation and melt prompt us to examine downstream changes in streamflow. The objective of this study is to understand how changes in precipitation phase and snowmelt timing alter the timing of surface water inputs (i.e. rainfall and snowmelt) and the partitioning of these inputs between evapotranspiration and streamflow. We used the Distributed Hydrology Soil Vegetation Model and Weather Research and Forecasting Model-based projections of future climatic conditions to simulate streamflow. New hydrological insights for the region Modeled annual streamflow decreased by 22\% for the period 2071\textendash 2100. Surface water inputs increased during winter when atmospheric water demand was relatively low. Subsequently, the winter-period partitioning of water (as rain or snowmelt) to streamflow (as opposed to evapotranspiration) increased, by 15\%, while partitioning to evapotranspiration decreased, effectively buffering what would have otherwise been a larger net streamflow decline associated with warming. Seasonal streamflow buffering is unique to snow-influenced systems, as the magnitude and timing of water released from snowpacks is sensitive to warming. This effect may diminish as warming drives snow-influenced systems toward rain-dominance, with implications for hydrological and ecological processes and water-resource management.},
  langid = {english},
  keywords = {Budyko,Hydroclimatology,Modeling,Snowmelt,Streamflow,Water},
  file = {/Users/jacktarricone/Zotero/storage/QZ6XJD46/Hale et al. - 2022 - Modeling streamflow sensitivity to climate warming.pdf}
}

@article{haleRecentDecreasesSnow2023,
  title = {Recent Decreases in Snow Water Storage in Western {{North America}}},
  author = {Hale, Katherine E. and Jennings, Keith S. and Musselman, Keith N. and Livneh, Ben and Molotch, Noah P.},
